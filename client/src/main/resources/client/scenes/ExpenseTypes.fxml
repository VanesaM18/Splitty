<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="428.0" prefWidth="364.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.ExpenseTypeCtrl">
   <children>
      <ListView fx:id="tags" layoutX="32.0" layoutY="71.0" prefHeight="313.0" prefWidth="300.0" />
      <Label layoutX="32.0" layoutY="35.0" text="Overview of all tags">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Button fx:id="addTag" layoutX="292.0" layoutY="35.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="40.0" onAction="#addTags">
         <graphic>
            <ImageView fitHeight="22.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../assets/plus-sign.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="228.0" layoutY="390.0" mnemonicParsing="false" onAction="#back" text="Save changes">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
